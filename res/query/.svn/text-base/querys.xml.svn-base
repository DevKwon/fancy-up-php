<?xml version="1.0" encoding="utf-8"?>
<querys>
	<tables>
		<authtoken>cp_authtoken</authtoken>
		<member>cp_member</member>
		<customers>cp_customers</customers>
		<uploadfiles>ax_uploadfiles</uploadfiles>
		<uploadfiles_temp>ax_uploadfiles_temp</uploadfiles_temp>
		<department>cp_department</department>
		<department_member>cp_department_member</department_member>
		<talk>cp_talk</talk>
		<talk_text>cp_talk_fulltext</talk_text>
		<talk_share>cp_talk_share</talk_share>
		<comment>cp_talk_comment</comment>
		<nfc>cp_nfc_chip</nfc>
		<my_nfc>cp_nfc_chip_my</my_nfc>
		<nfc_log>cp_nfc_chip_log</nfc_log>
		<taa>cp_taa</taa>
		<push_token>cp_push_token</push_token>
		
		<cd>game_cd</cd>
		<cdstage>game_stage</cdstage>
		<cdgroup>game_group</cdgroup>
		<cdmission>game_stage_mission</cdmission>
		<cduser>game_user</cduser>
		<cdlog>game_stage_mission_log</cdlog>
	</tables>
	<member>
		<member_list>SELECT uid,hp,name,job_position FROM cp_member WHERE uid='?'</member_list>
	</member>
	<talk>
		<list>SELECT t.uid,t.muid,t.update,t.reply_count FROM cp_talk t, (SELECT tuid FROM cp_talk_share WHERE mycuscode='?') s WHERE t.uid=s.tuid ORDER BY t.update DESC LIMIT 10</list>
		<list_1101>SELECT t.uid,t.muid,t.update,t.reply_count FROM cp_talk t, (SELECT tuid FROM cp_talk_share WHERE mycuscode='?') s WHERE t.uid=s.tuid AND t.uid&lt;? ORDER BY t.update DESC LIMIT 10</list_1101>
		<list_1100>SELECT t.uid,t.muid,t.update,t.reply_count FROM cp_talk t, (SELECT tuid FROM cp_talk_share WHERE mycuscode='?') s WHERE t.uid=s.tuid AND t.uid>? ORDER BY t.update DESC LIMIT 10</list_1100>
	</talk>
	<talk_search>
		<list>SELECT t.p_uid,t.description FROM cp_talk_fulltext t, (SELECT tuid,`update` FROM cp_talk_share WHERE mycuscode='?') s WHERE t.p_uid=s.tuid AND MATCH (t.description) AGAINST ('*?*' IN BOOLEAN MODE) ORDER BY s.update DESC LIMIT 30</list>
		<list_1101>SELECT t.p_uid,t.description FROM cp_talk_fulltext t, (SELECT tuid,`update` FROM cp_talk_share WHERE mycuscode='?') s WHERE t.p_uid=s.tuid AND t.p_uid&lt;? AND MATCH (t.description) AGAINST ('*?*' IN BOOLEAN MODE) ORDER BY s.update DESC LIMIT 30</list_1101>
		<list_1100>SELECT t.p_uid,t.description FROM cp_talk_fulltext t, (SELECT tuid,`update` FROM cp_talk_share WHERE mycuscode='?') s WHERE t.p_uid=s.tuid AND t.p_uid>? AND MATCH (t.description) AGAINST ('*?*' IN BOOLEAN MODE) ORDER BY s.update DESC LIMIT 30</list_1100>
	</talk_search>
	<comment>
		<list>SELECT cuid,muid,signdate,comment FROM cp_talk_comment WHERE puid='?' ORDER BY cuid DESC LIMIT 10</list>
		<list_1101>SELECT cuid,muid,signdate,comment FROM cp_talk_comment WHERE cuid&lt;? AND puid='?' ORDER BY cuid DESC LIMIT 10</list_1101>
		<list_1100>SELECT cuid,muid,signdate,comment FROM cp_talk_comment WHERE cuid>? AND puid='?' ORDER BY cuid ASC LIMIT 10</list_1100>
	</comment>
	<taa>
		<month>SELECT t.uid,t.muid,t.state,t.signdate FROM cp_taa t, (SELECT uid FROM cp_member WHERE pcus_code='?' AND uid='?') m WHERE (t.muid=m.uid) AND (?) ORDER BY t.signdate ASC</month>
		<query>SELECT * FROM ax_calendar WHERE id='?' AND del_date='1' AND uid=?</query>
	</taa>
</querys>